d_fclass_b1:
    config: 
      - check ISA:=regex(.*I.*D.*)
    opcode: 
      fclass.d: 0
    rs1: 
      <<: *all_fregs
    rd: 
      <<: *all_regs
    val_comb:
      abstract_comb:
        'ibm_b1(64, "fclass.d", 1)': 0
        
