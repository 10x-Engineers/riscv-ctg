flt_b1:
    config: 
      - check ISA:=regex(.*I.*D.*)
    opcode: 
      flt.d: 0
    rs1: 
      <<: *all_fregs
    rs2:
      <<: *all_fregs
    rd: 
      <<: *all_regs
    op_comb: 
      <<: *sfmt_op_comb
    val_comb:
      abstract_comb:
        'ibm_b1(64, "flt.d", 2)': 0
        
flt_b19:
    config: 
      - check ISA:=regex(.*I.*D.*)
    opcode: 
      flt.d: 0
    rs1: 
      <<: *all_fregs
    rs2:
      <<: *all_fregs
    rd: 
      <<: *all_regs
    op_comb: 
      <<: *sfmt_op_comb
    val_comb:
      abstract_comb:
        'ibm_b19(64, "flt.d", 2)': 0
