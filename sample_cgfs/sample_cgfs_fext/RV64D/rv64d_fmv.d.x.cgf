fmv.d.x_b25:
    config: 
      - check ISA:=regex(.*I.*F.*)
    opcode: 
      fmv.d.x: 0
    rs1: 
      <<: *all_fregs
    rd: 
      <<: *all_regs
    op_comb: 
      <<: *ifmt_op_comb
    val_comb:
      abstract_comb:
        'ibm_b25(64, "fmv.d.x", 1)': 0
        
fmv.d.x_b26:
    config: 
      - check ISA:=regex(.*I.*F.*)
    opcode: 
      fmv.d.x: 0
    rs1: 
      <<: *all_fregs
    rd: 
      <<: *all_regs
    op_comb: 
      <<: *ifmt_op_comb
    val_comb:
      abstract_comb:
        'ibm_b26(64, "fmv.d.x", 1)': 0

