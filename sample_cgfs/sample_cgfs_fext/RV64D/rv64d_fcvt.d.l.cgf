d_fcvt.d.l_b25:
    config: 
      - check ISA:=regex(.*I.*D.*)
    opcode: 
      fcvt.d.l: 0
    rs1: 
      <<: *all_regs
    rd: 
      <<: *all_fregs
    val_comb:
      abstract_comb:
        'ibm_b25(64, "fcvt.d.l", 1)': 0
        
d_fcvt.d.l_b26:
    config: 
      - check ISA:=regex(.*I.*D.*)
    opcode: 
      fcvt.d.l: 0
    rs1: 
      <<: *all_regs
    rd: 
      <<: *all_fregs
    val_comb:
      abstract_comb:
        'ibm_b26(64, "fcvt.d.l", 1)': 0

