aes64ds:
    config: 
      - check ISA:=regex(.*RV64.*I.*K.*)
      - check ISA:=regex(.*RV64.*I.*ZKn.*)
    opcode: 
      aes64ds: 0
    rs1: 
      <<: *all_regs
    rs2:
      <<: *all_regs
    rd:
      <<: *all_regs
    op_comb: 
      <<: *rfmt_op_comb
    val_comb:
      abstract_comb:
        'byte_count(64)': 0
        'uniform_random(20, 100, ["rs1","rs2"], [64, 64])': 0

